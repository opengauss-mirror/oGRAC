{
  "add node": {
    "handler": "ograc",
    "paramCheck": "",
    "description": "uds请求tasks.json填写规范"
  },
  "cmd": {
    "handler": "cmd",
    "paramCheck": "",
    "cmd": "ls -l | grep ${key1} ${key2}",
    "description": "shell指令tasks.json填写规范"
  },
  "ogctl sh": {
    "handler": "shell",
    "paramCheck": "",
    "filePath": "/tmp/og_om/ogmgr/shell_file_test/sh_test.sh",
    "sh_input": "${key1} ${key2}",
    "description": "调用sh脚本tasks.json填写规范"
  },
  "show cantina status": {
    "handler": "audit_py",
    "paramCheck": "",
    "filePath": "/tmp/og_om/ogmgr/py_file_test/py_test.py",
    "py_input": "",
    "description": "调用py脚本tasks.json填写规范"
  },
  "collection logs": {
    "handler": "py",
    "checkFunc": "collection_logs_checker.LogEnumCheck",
    "paramCheck": {
      "key1": {
        "required": true,
        "type": "int",
        "intString": true,
        "regexp": "^0$|^1$|^2$|^3$|^4$|^5$|^6$|^7$|^8$",
        "enum": "true|FALSE",
        "intRange": "1~5",
        "strLengthRange": "1~10",
        "nonEmptyStr": true,
        "StrList": true,
        "strListRange": "1~3",
        "intList": true,
        "intListRange": "1~3",
        "nonRepeatList": true,
        "arrayLength": "0~3"
      }
    },
    "filePath": "/opt/ograc/og_om/service/ogmgr/logs_collection/execute.py",
    "py_input": "-p ${log_dir} -t ${type}",
    "description": ""
  }
}